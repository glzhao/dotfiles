# Set default values for all following accounts.
defaults
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile ~/.msmtp.log

# Passwords are stored in keyring,
account work 
host mail.acc1.net
from me@acc1.com
tls_starttls off
port 465
auth on
user me@acc1.com

account lucienchao@gmail.com
from lucienchao@gmail.com
host smtp.gmail.com
# tls_certcheck off
tls_min_dh_prime_bits 512
auth on
port 587
user lucienchao
# password secret
# passwordeval python ~/.mutt/offlineimap.py
passwordeval python -c "import keyring; print keyring.get_password('smtp.gmail.com', 'lucienchao')"

account glzhao1@gmail.com
from glzhao1@gmail.com
host smtp.gmail.com
# tls_certcheck off
tls_min_dh_prime_bits 512
auth on
port 587
user glzhao1
# passwordeval python ~/.mutt/offlineimap.py
passwordeval python -c "import keyring; print keyring.get_password('smtp.gmail.com', 'glzhao1')"

# Set a default account
account default: lucienchao@gmail.com 
